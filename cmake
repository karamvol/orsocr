cmake -D CMAKE_INSTALL_PREFIX=/usr/local \
      -D WITH_CUDA=OFF \
      -D WITH_QT=OFF \
      -D WITH_OPENGL=ON \
      -D OPENCV_EXTRA_MODULES_PATH=/home/mctrjalloh/sapienscube/installations/orsocr/opencv_contrib-3.4.1/modules \
      -D ENABLE_FAST_MATH=ON \ 
      -D CUDA_FAST_MATH=ON \
      -D CUDA_NVCC_FLAGS="-D_FORCE_INLINES" \
      -D WITH_CUBLAS=ON \
      -D BUILD_OPENCV_PYTHON3=ON \
      -D PYTHON3_EXECUTABLE=/home/mctrjalloh/anaconda3/bin/python 
      -D PYTHON3_INCLUDE_PATH=/home/mctrjalloh/anaconda3/include/python3.6m 
      -D PYTHON3_LIBRARIES=/home/mctrjalloh/anaconda3/lib/python3.6/site-packages \
      -D OPENCV_ENABLE_NONFREE=ON \
      -D WITH_TBB=ON \ 
      -D WITH_V4L=ON \
      -D INSTALL_C_EXAMPLES=OFF \ 
      -D BUILD_SHARED_LIBS=ON ..
